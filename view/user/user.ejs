    <script>
        function cartLoyaltyPoints() {
            var cartHUFTTotal = $("#cartHuftLtyInput").val();
            var cartNonHUFTTotal = $("#cartNonHuftLtyInput").val();
            if (window.sessionStorage.getItem("ltyCS_CurrentTier")) {
                //For Logged IN Users
                //$(".LTY_Cart p").hide();
                var cartUserTier = window.sessionStorage.getItem("ltyCS_CurrentTier");
