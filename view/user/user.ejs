                    var cartHPointMlt = 0.2;
                    var cartNHPointsMlt = 0.1;
                } else if (cartUserTier == "Silver") {
                    var cartHPointMlt = 0.3;
                    var cartNHPointsMlt = 0.2;
                } else if (cartUserTier == "Gold") {
                    var cartHPointMlt = 0.4;
                    var cartNHPointsMlt = 0.3;
                } else {
                    var cartHPointMlt = 0.1;
                    var cartNHPointsMlt = 0.1;
                }
            } else {
                //For Non Logged In user
                var cartHPointMlt = 0.4;
                var cartNHPointsMlt = 0.3;
            }

            var cartHUFTTotal = cartHUFTTotal * cartHPointMlt;
            var cartNonHUFTTotal = cartNonHUFTTotal * cartNHPointsMlt;
            var CartTotalLTYPoints = cartHUFTTotal + cartNonHUFTTotal;
            //CartTotalLTYPoints = CartTotalLTYPoints.toFixed(2);
